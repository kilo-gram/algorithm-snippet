			"\t\tif (out[i] - in[i] == 1) cnt2++, root = i;",
			"\t\tif (abs(in[i] - out[i]) > 1) ok = 0;",
			"\t}",
			"\tif (cnt1 > 1 || cnt2 > 1) ok = 0;",
			"\tif (!ok) return 0;",
			"\tif (root == 0) {",
			"\t\tfor (int i = 1; i <= n; i++) if (out[i]) root = i;",
			"\t}",
			"\tif (root == 0) return 1; //empty graph",
			"\tdfs(root);",
			"\tif (ans.size() != edges + 1) return 0; //connectivity",
